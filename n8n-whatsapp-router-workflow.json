    {
      "parameters": {
        "jsCode": "// Error handling and logging\nconst error = $input.all()[0];\n\nconst errorResponse = {\n  success: false,\n  message: 'Error processing WhatsApp webhook',\n  error: error.error?.message || 'Unknown error',\n  timestamp: new Date().toISOString()\n};\n\n// Log error (you can send this to your logging service)\nconsole.error('WhatsApp Webhook Error:', errorResponse);\n\nreturn errorResponse;"
      },
